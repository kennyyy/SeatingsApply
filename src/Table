	CREATE TABLE MEMBERS (
	ID	VARCHAR2(50)	NOT NULL,
	PW	VARCHAR2(50)	NULL,
	NAME	VARCHAR2(50)	NULL,
	EMAIL	VARCHAR2(255)	NULL,
	ADDRESS	VARCHAR2(255)	NULL,
	AGE	NUMBER(5)	NULL,
	GENDER	CHAR(1)	NULL,
	REGDATE	DATE	NULL,
	MASTER	CHAR(1)	NULL,
	LIMITROOM	NUMBER(5)	NULL
);

CREATE TABLE OPTIONS (
	ROOMNUMBER	NUMBER(5)	NULL,
	NUMCOUNT	NUMBER(5)	NULL,
	DEADLINE	DATE	NULL,
	WIDTH	VARCHAR(255)	NULL,
	LENGTH	VARCHAR(255)	NULL,
	ID2	VARCHAR2(50)	NOT NULL
);

CREATE TABLE JOINS (
	USERID	VARCHAR2(50)	NOT NULL,
	ROOMNUMBER	NUMBER(5)	NULL,
	WINNING	CHAR(1)	NULL
);

CREATE TABLE SEAT (
	SEAT	VARCHAR2(10)	NOT NULL,
	ROOMNUMBER	NUMBER(5)	NULL
);


ALTER TABLE MEMBERS ADD CONSTRAINT PK_MEMBERS PRIMARY KEY (
	ID
);

ALTER TABLE OPTIONS ADD CONSTRAINT PK_OPTIONS PRIMARY KEY (
	ROOMNUMBER
);

ALTER TABLE JOINS ADD CONSTRAINT PK_JOINS PRIMARY KEY (
	USERID,
	ROOMNUMBER
);

ALTER TABLE SEAT ADD CONSTRAINT PK_SEAT PRIMARY KEY (
	SEAT,
	ROOMNUMBER
);

ALTER TABLE JOINS ADD CONSTRAINT FK_MEMBERS_TO_JOINS_1 FOREIGN KEY (
	USERID
)
REFERENCES MEMBERS (
	ID
);

ALTER TABLE JOINS ADD CONSTRAINT FK_OPTIONS_TO_JOINS_1 FOREIGN KEY (
	ROOMNUMBER
)
REFERENCES OPTIONS (
	ROOMNUMBER
);

ALTER TABLE SEAT ADD CONSTRAINT FK_OPTIONS_TO_SEAT_1 FOREIGN KEY (
	ROOMNUMBER
)
REFERENCES OPTIONS (
	ROOMNUMBER
);

